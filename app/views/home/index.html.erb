<h1>Home#index</h1>


      <%= form_tag("/home", method: "post") do %>
        <%= label_tag(:long_link, "Short Link for:") %>
        <%= text_field_tag(:long_link) %>

        <%= submit_tag("Generate") %>
      <% end %>




<% if user_signed_in? %>
<%= current_user.email %>

  <li>
  <%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
  </li>
<% else %>
  <li>
  <%= link_to('Login', new_user_session_path)  %>  
  </li>
<% end %>