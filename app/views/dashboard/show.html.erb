<p id="notice"><%= notice %></p>

<h1>DASHBOARD</h1>






<table class="table table-striped table-responsive">
  <thead>
    <tr>
      <th>URL</th>
      <th>Category</th>
      <th>Key</th>
      <th>times accessed</th>
      <th>last accessed</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @shortened_urls.each do |link| %>
      <tr>

        

        <td><%= link.url %></td>
        <td><%= link.category %></td>

        <td><%= link.unique_key %></td>

        <td><%= link.use_count %></td>
        <% if link.use_count > 0 %>
          <td><%= link.updated_at.strftime("%F") %></td>
          <% else %>
            <td></td>
        <% end %>



      <% if link.owner != nil %>

          <td><%= link.owner.firstname %></td>
          <td><%= link.owner.lastname %></td>

          <% else %>
            <td></td>
      <% end %>


      <% if link.owner.company != nil %>

          <td><%= link.owner.company.name %></td>
          <% else %>
            <td></td>
      <% end %>


    



      </tr>
    <% end %>
  </tbody>
</table>



<%= paginate @shortened_urls %>



<br>
